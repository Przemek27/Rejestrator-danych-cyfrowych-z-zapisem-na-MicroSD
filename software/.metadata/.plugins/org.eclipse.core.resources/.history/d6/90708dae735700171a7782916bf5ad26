#ifndef SDCARD_H_
#define SDCARD_H_

#include "alt_types.h"

void delay(void);

/*@return 0 - Initialization failed
 * 		  1 - Initialization successful*/
alt_u8 sdCard_Init(void);

/* @return -  response from SD Card
 * @param command - command send to SD Card
 * @param response_length - response length in bytes*/
alt_32* sdCard_SendCommand(alt_u8 *command, alt_u8 response_length);

#endif /* SDCARD_H_ */
